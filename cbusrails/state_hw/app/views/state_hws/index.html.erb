<p id="notice"><%= notice %></p>

<h1>State Hws</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Capital</th>
      <th>Population</th>
      <th>Area</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @state_hws.each do |state_hw| %>
      <tr>
        <td><%= state_hw.name %></td>
        <td><%= state_hw.capital %></td>
        <td><%= state_hw.population %></td>
        <td><%= state_hw.area %></td>
        <td><%= link_to 'Show', state_hw %></td>
        <td><%= link_to 'Edit', edit_state_hw_path(state_hw) %></td>
        <td><%= link_to 'Destroy', state_hw, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New State Hw', new_state_hw_path %>
